{
  "name": "efile",
  "version": "0.0.1",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "~3.1.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.5",
    "babel-core": "~6.23.1",
    "babel-eslint": "~7.1.1",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-es2015": "~6.22.0",
    "babelify": "~7.3.0",
    "bootswatch": "^4.0.0",
    "browser-sync": "~2.18.0",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.14",
    "del": "~2.2.2",
    "directory-tree": "^2.1.0",
    "envify": "~4.0.0",
    "foldero": "~0.1.1",
    "glob": "~7.1.1",
    "gulp": "~3.9.1",
    "gulp-changed": "~2.0.0",
    "gulp-cssnano": "~2.1.2",
    "gulp-data": "~1.2.1",
    "gulp-eslint": "~3.0.1",
    "gulp-htmlmin": "~3.0.0",
    "gulp-if": "~2.0.2",
    "gulp-imagemin": "~3.1.1",
    "gulp-load-plugins": "~1.5.0",
    "gulp-notify": "~3.0.0",
    "gulp-plumber": "~1.1.0",
    "gulp-postcss": "~6.3.0",
    "gulp-pug": "~3.3.0",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~3.1.0",
    "gulp-sourcemaps": "~2.4.1",
    "gulp-uglify": "~2.0.1",
    "gulp-util": "~3.0.8",
    "imagemin-pngquant": "~5.0.0",
    "imagemin-svgo": "~5.2.0",
    "js-yaml": "~3.8.1",
    "lodash": "~4.17.4",
    "minimist": "~1.2.0",
    "pug": "~2.0.3",
    "rimraf": "~2.6.1",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.9.0 "
  },
  "scripts": {
    "clean-deps": "rimraf node_modules",
    "clean": "gulp clean",
    "lint": "gulp eslint",
    "serve:prod": "NODE_ENV=development gulp serve",
    "serve": "NODE_ENV=production gulp serve --production",
    "build": "NODE_ENV=production gulp --production"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "browser": {
    "jquery": "jquery"
  },
  "browserify-shim": {
    "jquery": "$",
    "three": "global:THREE"
  },
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "environment": "DEVELOPMENT",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "*.js",
      "css": "*.{sass,scss}"
    }
  }
}
